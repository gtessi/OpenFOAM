FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 0 0 0 0 0];

internalField   uniform 2.5e-05;

boundaryField
{
    bottom
    {
        type            calculated;
        value           nonuniform 0();
    }
    top
    {
        type            calculated;
        value           nonuniform 0();
    }
    left
    {
        type            calculated;
        value           nonuniform 0();
    }
    right
    {
        type            calculated;
        value           nonuniform 0();
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 2.5e-05;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 2.5e-05;
    }
}


